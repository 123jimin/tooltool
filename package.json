{
  "name": "@jiminp/tooltool",
  "version": "0.3.1",
  "description": "Simple Utility Library for TypeScript",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.spec.*",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "prepare": "npm run build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build-docs": "typedoc",
    "build-docs:watch": "typedoc --watch",
    "lint": "eslint src/",
    "pretest": "pnpm build",
    "test": "mocha \"dist/**/*.spec.js\" --parallel"
  },
  "keywords": [
    "utility"
  ],
  "author": "Jimin Park",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/123jimin/tooltool.git"
  },
  "bugs": {
    "url": "https://github.com/123jimin/tooltool/issues"
  },
  "homepage": "https://github.com/123jimin/tooltool",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@jiminp/eslint-config": "^1.1.2",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "chai": "^6.2.2",
    "eslint": "^10.0.0",
    "mocha": "^11.7.5",
    "rimraf": "^6.1.3",
    "typedoc": "^0.28.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}