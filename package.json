{
  "name": "@jiminp/tooltool",
  "version": "0.2.0",
  "description": "Simple Utility Library for TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.spec.*",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "clean": "npx rimraf dist",
    "prepare": "npm run build",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "lint": "npx eslint src/",
    "pretest": "pnpm build",
    "test": "npx mocha \"dist/**/*.spec.js\""
  },
  "keywords": [
    "utility"
  ],
  "author": "Jimin Park",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/123jimin/tooltool.git"
  },
  "bugs": {
    "url": "https://github.com/123jimin/tooltool/issues"
  },
  "homepage": "https://github.com/123jimin/tooltool",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "chai": "^6.2.0",
    "eslint": "^9.37.0",
    "mocha": "^11.7.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  }
}
